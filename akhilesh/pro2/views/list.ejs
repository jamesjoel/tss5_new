<html>

<head>
    <title>Home Page</title>
    <link type="text/css" rel="stylesheet" href="/css/bootstrap.css" />
    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap.bundle.js"></script>
    <style>
        body{
            background-color: #ABAB12;
        }
    </style>
</head>

<body>
    <div class="container">
       


        <table align="center" border="1" cellpadding="10" cellspacing="0" class="table table-dark table-hover table-bordered">

            <tr>
                <th>S.No.</th>
                <th>Name</th>
                <th>City</th>
                <th>Age</th>
            </tr>

            <% stu.forEach(function(x, n){ %>
                <tr>
                    <td><%= n+1 %></td>
                    <td><%= x.name %></td>
                    <td><%= x.city %></td>
                    <td><%= x.age %></td>
                </tr>



            <% }) %>


       


        </table>
    </div>
</body>

</html>