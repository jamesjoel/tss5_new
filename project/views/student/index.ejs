<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1>Student Page</h1>
        </div>

    </div>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-dark table-hover table-bordered">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Age</th>
                    <th>City</th>
                </tr>
                <% result.forEach(function(student, index){ %>
                    <tr>
                        <td><%= index+1 %></td>
                        <td><%= student.name %></td>
                        <td><%= student.age %></td>
                        <td><%= student.city %></td>
                    </tr>
                <% }); %>
            </table>
        </div>
    </div>
</div>