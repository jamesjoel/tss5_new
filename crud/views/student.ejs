<div class="container">
    <h2>This is Student Page</h2>
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <table class="table table-dark table-hover table-bordered table-striped">
                <tr>
                    <th>S.No.</th>
                    <th>Full Name</th>
                    <th>Age</th>
                    <th>City</th>
                    <th>Delete</th>
                    <th>Edit</th>
                </tr>
                <% result.forEach(function(x, n){ %>
                    <tr>
                        <td><%= n+1 %></td>
                        <td><%= x.name %></td>
                        <td><%= x.age %></td>
                        <td><%= x.city %></td>
                        <td><a href="/delete/<%= x._id %>" class="btn btn-sm btn-danger">Delete</a></td>
                        <td><a href="/edit/<%= x._id %>" class="btn btn-sm btn-info">Edit</a></td>
                    </tr>
                <% }) %>


            </table>
        </div>
    </div>
</div>