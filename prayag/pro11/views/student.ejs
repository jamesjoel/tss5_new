<div class="col-md-8 offset-md-2">
	<table class="table table-hover table-dark table-bordered">
		<tr>
			<th>S.No</th>
			<th>Name</th>
			<th>Age</th>
			<th>City</th>
			<th>Delete</th>
			<th>Edit</th>
		</tr>
		<% result.forEach(function(x,n){ %>
		<tr>
			<td><%= n+1 %></td>
			<td><%= x.name %></td>
			<td><%= x.age %></td>
			<td><%= x.city %></td>
			<td><a href="/delete/<%= x._id %>" class="btn btn-danger btn-sm">Delete</a></td>
			<td><a href="/edit/<%= x._id %>" class="btn btn-success">Edit</a></td>
		</tr>

	   <%  }) %>
	
	</table>
</div>