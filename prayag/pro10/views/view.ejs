<div class="row">
	<div class="col-md-8 offset-md-2">
		<table class="table table-hover table-bordered table-dark table-striped">
			<tr>
				<th>S.No</th>
				<th>Name</th>
				<th>Age</th>
				<th>Department</th>
				<th>City</th>

			</tr>
			<% result.forEach(function(x,n){ %>
				<tr>
				<td><%= n+1 %></td>
				<td><%= x.name %></td>
				<td><%= x.age %></td>
				<td><%= x.department %></td>
				<td><%= x.city %></td>
			</tr>
	        <% }) %>
		</table>
	</div>
</div>
