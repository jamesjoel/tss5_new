                            <th>Age</th>
                            <th>City</th>
                        </tr>
                        <% userdata.forEach(function(x,n){ %> 
                        <tr>
                            <td><%= n+1 %></td>
                            <td><%= x.name %></td>
                            <td><%= x.age %></td>
                            <td><%= x.city %></td>
                        </tr>

                      <%  }) %>
                        
